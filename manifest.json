{
	"manifest_version": 2,

	"name": "Thingiverse",
	"description": "Keep up to date with all things Thingiverse.",
	"version": "1.0",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

	"permissions": [
		"storage",
		"https://thingiverse.com/",
		"http://thingiverse.dev/"
		],
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "index.html"
	}
}
